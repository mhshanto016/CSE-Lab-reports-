#include <stdio.h>
int isPal(int num) {
    int original_Num = num;
    int revNum = 0;
    while (num > 0) {
        int digit = num % 10;
        revNum = revNum * 10 + digit;
        num /= 10;
    }
    return original_Num == revNum;
}
int main() {
    int num;
    scanf("%d", &num);
    if (isPal(num)) {
        printf("%d is a palindrome.\n", num);
    } else {
        printf("%d is not a palindrome.\n", num);
    } return 0;}
